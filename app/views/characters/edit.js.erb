$("#character-show-modal").modal('toggle');
$(".creatures").after('<%= escape_javascript(render "edit") %>');
$("#character-edit-modal").modal('toggle');
 $("#edit-close").click(function() {
    $("#character-edit-modal").modal('toggle');
    $("#character-show-modal").modal('toggle');
  })
 $("#character-form-close").click(function() {
    $("#character-edit-modal").modal('toggle');
    $("#character-index-modal").modal('toggle');
  })
